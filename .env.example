# ====================================
# DartWing Mobile Gateway Configuration
# ====================================
# Copy this file to .env and configure your gateways

# How many gateways are configured
GATEWAY_COUNT=2

# ====================================
# Gateway 1 - Production
# ====================================
GATEWAY_1_NAME=Production
GATEWAY_1_KEYCLOAK_URL=https://your-keycloak.com/realms/YourRealm
GATEWAY_1_DARTWING_URL=https://your-dartwing-api.com
GATEWAY_1_HEALTHCARE_URL=https://your-frappe.com
GATEWAY_1_ENABLED=true
GATEWAY_1_IS_DEFAULT_RELEASE=true
GATEWAY_1_IS_DEFAULT_DEBUG=false

# ====================================
# Gateway 2 - QA
# ====================================
GATEWAY_2_NAME=QA
GATEWAY_2_KEYCLOAK_URL=https://qa.your-keycloak.com/realms/YourRealm
GATEWAY_2_DARTWING_URL=https://qa.your-dartwing-api.com
GATEWAY_2_HEALTHCARE_URL=https://qa.your-frappe.com
GATEWAY_2_ENABLED=true
GATEWAY_2_IS_DEFAULT_RELEASE=false
GATEWAY_2_IS_DEFAULT_DEBUG=true
